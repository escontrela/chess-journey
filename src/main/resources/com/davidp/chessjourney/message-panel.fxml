<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<fx:root prefHeight="75.0" prefWidth="502.0" styleClass="note-panel" stylesheets="@custom-styles.css" type="Pane" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="lblMessage" layoutX="70.0" layoutY="30.0" styleClass="note-label" text="This is a note that you have to confirm, or not?" textFill="#8389de">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <ImageView fx:id="imgIcon" fitHeight="27.0" fitWidth="31.0" layoutX="21.0" layoutY="26.0" preserveRatio="true" styleClass="note-icon">
         <image>
            <Image url="@img-white/chat_40dp_white.png" />
         </image>
      </ImageView>
      <Button fx:id="btCancel" alignment="BASELINE_CENTER" layoutX="435.0" layoutY="19.0" mnemonicParsing="false" onAction="#buttonAction" prefHeight="39.0" prefWidth="41.0" styleClass="button-regular" stylesheets="@custom-styles.css" textFill="WHITE">
         <font>
            <Font name="Alatsi Regular" size="18.0" />
         </font>
         <graphic>
            <ImageView fitHeight="16.0" fitWidth="25.0" preserveRatio="true">
               <image>
                  <Image url="@img-white/close_18dp_white.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="btOk" alignment="BASELINE_CENTER" layoutX="384.0" layoutY="19.0" mnemonicParsing="false" onAction="#buttonAction" prefHeight="39.0" prefWidth="41.0" styleClass="button-regular" stylesheets="@custom-styles.css" textFill="WHITE">
         <font>
            <Font name="Alatsi Regular" size="18.0" />
         </font>
         <graphic>
            <ImageView fitHeight="16.0" fitWidth="25.0" preserveRatio="true">
               <image>
                  <Image url="@img-white/check_18dp_white.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
   </children>
</fx:root>